{
  "version": 2,
  "builds": [
    { "src": "index.html",   "use": "@vercel/static" },
    { "src": "script.js",    "use": "@vercel/static" },
    { "src": "style.css",    "use": "@vercel/static" },
    { "src": "sw.js",        "use": "@vercel/static" },
    { "src": "manifest.json","use": "@vercel/static" },
    { "src": "images/**/*",  "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/$1" }
  ]
}